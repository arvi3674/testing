pipeline{

    agent { label 'linux-slave1' }

    environment {
        dockerImage = ''
    }

    stages {
        
        stage("Display parent git url") {
            when {
                
                expression { GIT_URL == 'https://github.com/arvi3674/testing.git' }
            }
            steps{
                sh "echo WORKING FINE"
            }
        }
        
    }
}